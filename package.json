{
  "name": "roomme",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run setup:client && npm run setup:server && npm run build:client",
    "setup:client": "cd roomme.client && npm i",
    "setup:server": "cd roomme.server && npm i",
    "build:client": "npm run --prefix roomme.client build",
    "start": "node roomme.server/index.js"
  },
  "engines": {
    "node": "^14.15.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vue2-interact": "0.0.3"
  }
}
